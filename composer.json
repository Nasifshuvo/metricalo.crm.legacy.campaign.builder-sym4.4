{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "2.3.2",
        "doctrine/orm": "2.7.5",
        "friendsofsymfony/jsrouting-bundle": "2.8",
        "gedmo/doctrine-extensions": "3.2.0",
        "giggsey/libphonenumber-for-php": "8.13",
        "knplabs/doctrine-behaviors": "2.0.4",
        "knplabs/knp-snappy-bundle": "1.8",
        "sensio/framework-extra-bundle": "6.2.*",
        "sonata-project/translation-bundle": "^2.4",
        "stof/doctrine-extensions-bundle": "1.7",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.4.*",
        "symfony/yaml": "4.4.*",
        "twig/cssinliner-extra": "^3.7",
        "twig/extra-bundle": "^3.7",
        "twig/inky-extra": "^3.7",
        "twig/markdown-extra": "^3.7",
        "twig/twig": "^3.11",
        "ext-json": "*"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Common\\": "lib/CommonBundle/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {

    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
